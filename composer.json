{
  "name": "tourze/wechat-official-account-bundle",
  "description": "公众号基础能力",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "nesbot/carbon": "^2.72 || ^3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/http-client-contracts": "^2.5 | ^3.0",
    "tourze/http-client-bundle": "0.1.*",
    "tourze/lock-service-bundle": "0.0.*",
    "tourze/doctrine-track-bundle": "0.1.*",
    "tourze/doctrine-snowflake-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "WechatOfficialAccountBundle\\": "src"
    }
